package 연습;

class Solution {
	 public int[] main(int[] num_list) {
		 	int[] num_list1 = {1,2};
	        int[] answer = new int[2];

	        for(int i = 0 ; i < num_list.length ; i++){
	            if(num_list[i]%2==0){
	                answer[0]++;
	            }else{
	                answer[1]++;
	            }
	        }

	        return answer;
	    }
}
