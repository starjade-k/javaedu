package 연습;

public class Solution {
	static int [] arr;
	public int[] solution(int[] num_list) {
        int[] answer = new int[2];
        int [] arr = {1,2,3,4,5};
        for(int i = 0 ; i < num_list.length ; i++){
            if(num_list[i]%2==0){
                answer[0]++;
            }else{
                answer[1]++;
            }
        }

        return answer;
    }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Solution so = new Solution();
		so.solution(arr);
	}

}
